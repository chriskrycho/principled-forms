{
  "name": "@olo/principled-forms",
  "version": "0.1.3",
  "description": "Principled form handling in TypeScript.",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "author": "Chris Krycho",
  "license": "MIT",
  "dependencies": {
    "true-myth": "2.0.0"
  },
  "devDependencies": {
    "ember-cli": "3.1.2",
    "prettier-eslint": "8.8.1",
    "rimraf": "2.6.2",
    "shelljs": "^0.8.2",
    "typescript": "^2.8.3",
    "typescript-eslint-parser": "15.0.0"
  },
  "main": "ember-addon.js",
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "main": "ember-addon.js"
  },
  "scripts": {
    "ts:src": "yarn tsc",
    "ts:typedefs": "yarn tsc --declaration --outDir . --emitDeclarationOnly",
    "ts:all": "yarn ts:src && yarn ts:typedefs",
    "clean": "rimraf *.d.ts field/*.d.ts dist",
    "build": "yarn clean && yarn ts:all",
    "prepublishOnly": "yarn build",
    "postpublish": "yarn clean"
  },
  "files": [
    "*.d.ts",
    "field/*.d.ts",
    "dist"
  ]
}
